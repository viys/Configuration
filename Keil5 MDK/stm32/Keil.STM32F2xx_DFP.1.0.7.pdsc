  <conditions>
    <!-- conditions are dependecy rules that can apply to a component or an individual file -->
    <condition id="Compiler ARM">
      <!-- conditions selecting ARM Compiler -->
      <require Tcompiler="ARMCC"/>
    </condition>

    <condition id="STM32F20x">
      <description>STMicroelectronics STM32F2 Series devices</description>
      <require Dname="STM32F20[57]??"/>
    </condition>
    <condition id="STM32F21x">
      <description>STMicroelectronics STM32F21x Devices</description>
      <require Dname="STM32F21[57]??"/>
    </condition>

    <condition id="STM32F2xx CMSIS Device">
      <!-- conditions selecting Devices -->
      <description>STMicroelectronics STM32F2 Series devices and CMSIS-CORE</description>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Dvendor="STMicroelectronics:13" Dname="STM32F2????"/>
    </condition>

    <condition id="STM32F2 STDPERIPH">
      <description>STMicroelectronics STM32F2 Standard Peripherals Drivers requirements</description>
      <require condition="STM32F2xx CMSIS Device"/>
      <require Cclass="Device" Cgroup="StdPeriph Drivers" Csub="Framework"/>
    </condition>

    <condition id="STM32F2 STDPERIPH RCC">
      <description>STMicroelectronics STM32F2 Standard Peripherals Drivers with RCC</description>
      <require condition="STM32F2 STDPERIPH"/>
      <require Cclass="Device" Cgroup="StdPeriph Drivers" Csub="RCC"/>
    </condition>

    <condition id="STM32F21x STDPERIPH RCC">
      <description>STMicroelectronics STM32F2 Standard Peripherals Drivers with RCC</description>
      <require condition="STM32F21x"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="Device" Cgroup="StdPeriph Drivers" Csub="Framework"/>
      <require Cclass="Device" Cgroup="StdPeriph Drivers" Csub="RCC"/>
    </condition>

    <condition id="STM32F2xx CMSIS GPIO">
      <description>STMicroelectronics STM32F2 Series GPIO Driver with CMSIS</description>
      <require Cclass ="CMSIS" Cgroup="CORE"/>
      <require Dvendor="STMicroelectronics:13" Dname="STM32F2????"/>
      <require Cclass ="Device" Cgroup="GPIO" />
    </condition>

    <condition id="STM32F2xx CMSIS GPIO DMA">
      <description>STMicroelectronics STM32F2 Series GPIO and DMA Driver with CMSIS-CORE</description>
      <require Cclass ="CMSIS" Cgroup="CORE"/>
      <require Dvendor="STMicroelectronics:13" Dname="STM32F2????"/>
      <require Cclass ="Device" Cgroup="GPIO" />
      <require Cclass ="Device" Cgroup="DMA" />
    </condition>

    <condition id="STM32F2xx CMSIS RTOS GPIO">
      <description>STMicroelectronics STM32F2 Series GPIO Driver with CMSIS and RTOS</description>
      <require Cclass ="CMSIS" Cgroup="CORE"/>
      <require Cclass ="CMSIS" Cgroup="RTOS"/>
      <require Dvendor="STMicroelectronics:13" Dname="STM32F2????"/>
      <require Cclass ="Device" Cgroup="GPIO" />
    </condition>

    <condition id="STM32F2xx CMSIS RTOS GPIO DMA">
      <description>STMicroelectronics STM32F2 Series GPIO and DMA Driver with CMSIS and RTOS</description>
      <require Cclass ="CMSIS" Cgroup="CORE"/>
      <require Cclass ="CMSIS" Cgroup="RTOS"/>
      <require Dvendor="STMicroelectronics:13" Dname="STM32F2????"/>
      <require Cclass ="Device" Cgroup="GPIO" />
      <require Cclass ="Device" Cgroup="DMA" />
    </condition>

    <condition id="STM32F2xx CMSIS RTOS I2C">
      <description>STMicroelectronics STM32F2 Series I2C Driver with CMSIS and RTOS</description>
